<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="org.dj.dao.ResumeDao">
	<cache eviction="LRU" type="org.dj.common.cache.MybatisRedisCache" />

	<select id="queryResumeByUserId" parameterType="string" resultType="resumeVo">
		select a.name,b.username from resume a,user b where a.userId=b.userId and a.userId=#{value}
	</select>

</mapper>
